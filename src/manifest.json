{
  "manifest_version": 2,
  "name": "GIFHub",
  "short_name": "GIFHub",
  "version": "0.0.1",
  "description": "Stuff and things",
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "author": "Maneesh Chiba",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
